      print(Value)
   end,
})
local Slider = Tab:CreateSlider({
   Name = "Walk speed",
   Info = "changes your walk speed.",
   Range = {0, 100},
   Increment = 10,
   Suffix = "Walk speed",
   CurrentValue = 10,
   Flag = "Slider1", 
   Callback = function(Value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = tonumber(Value)
      print(Value)
   end,
})
